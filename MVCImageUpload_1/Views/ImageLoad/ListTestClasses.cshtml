@model ImageLoadVM
@{
    ViewBag.Title = "ListTestClasses";
}



@foreach (TestClass item in Model.TestClasses)
{
    <img src="@item.ImagePath" width="300" height="300" />
}

<table class="table table-hover">
    <tr>
        <th>Isim</th>
        <th>Resim</th>
    </tr>

    @foreach (TestClass item in Model.TestClasses)
    {
       <tr>
           <td>@item.Name</td>
           <td><img src="@item.ImagePath" width="30" height="30" />    </td>
       </tr>
    }
</table>



